<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaCarne"
    android:background="#383333">

    <Button
        android:id="@+id/button_file"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_above="@+id/button_picanha"
        android:layout_centerHorizontal="true"
        android:backgroundTint="#86ffffff"
        android:text="FILÃ‰"
        android:textColor="#000000"
        android:textSize="@dimen/texto_file"
        android:textStyle="bold" />

    <Button
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:text="PICANHA"
        android:id="@+id/button_picanha"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:textSize="@dimen/texto_picanha"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/imageView17"
        android:layout_alignStart="@+id/imageView17"
        android:textStyle="bold" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:id="@+id/imageView17"
        android:src="@drawable/g"
        android:layout_below="@+id/button_picanha"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/buttonCarneVoltar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="11dp"
        android:background="@drawable/abc_vector_test" />

</RelativeLayout>
