<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaPeixe"
    android:background="#383333">

    <Button
        android:id="@+id/button_palito"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="54dp"
        android:backgroundTint="#86ffffff"
        android:text="PALITO DE TILAPIA"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_pedaco"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:layout_above="@+id/button_bacalhau"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:backgroundTint="#86ffffff"
        android:text="PEDAÇOS DE TILAPIA"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_bolinho"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_palito"
        android:backgroundTint="#86ffffff"
        android:text="BOLINHO DE PEIXE"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_bacalhau"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/button_bolinho"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:backgroundTint="#86ffffff"
        android:text="BACALHAU"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_medalhao_prolone"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/button_medalao_bacon"
        android:backgroundTint="#86ffffff"
        android:text="MEDALHAO PROVOLONE"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_medalao_bacon"
        android:layout_width="165dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/button_bacalhau"
        android:layout_alignStart="@+id/button_bacalhau"
        android:layout_below="@+id/button_bacalhau"
        android:backgroundTint="#86ffffff"
        android:text="MEDALHÃO BACON"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_below="@+id/button_medalhao_prolone"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        android:src="@drawable/b" />

    <Button
        android:id="@+id/buttonPeixeVoltar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/abc_vector_test" />

</RelativeLayout>
