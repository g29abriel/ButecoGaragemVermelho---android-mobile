<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaFrango"
    android:background="#383333">

    <Button
        android:id="@+id/button_passarinho"
        android:layout_width="185dp"
        android:layout_height="90dp"
        android:layout_alignLeft="@+id/button_barbecue"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/button_barbecue"
        android:layout_marginTop="70dp"
        android:backgroundTint="#86ffffff"
        android:text="PASSARINHO"
        android:textColor="#000000"
        android:textSize="@dimen/texto_passarinho"
        android:textStyle="bold" />

    <Button
        android:layout_width="185dp"
        android:layout_height="90dp"
        android:text="BARBECUE"
        android:id="@+id/button_barbecue"
        android:textSize="@dimen/texto_barbecue"
        android:layout_below="@+id/button_passarinho"
        android:layout_alignLeft="@+id/imageView9"
        android:layout_alignStart="@+id/imageView9"
        android:backgroundTint="#86ffffff"
        android:textColor="#000000"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_below="@+id/button_barbecue"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:src="@drawable/h" />

    <Button
        android:id="@+id/buttonFrangoVoltar"
        android:layout_width="52dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/abc_vector_test" />

</RelativeLayout>
