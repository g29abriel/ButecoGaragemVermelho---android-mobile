<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaPetiscos"
    android:background="#383333">

    <Button
        android:id="@+id/button_salame"
        android:layout_width="165dp"
        android:layout_height="90dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="41dp"
        android:backgroundTint="#86ffffff"
        android:text="SALAME"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_codorna"
        android:layout_width="165dp"
        android:layout_height="90dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_salame"
        android:backgroundTint="#86ffffff"
        android:text="OVO DE CODORNA"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_azeitona"
        android:layout_width="165dp"
        android:layout_height="90dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button_salame"
        android:backgroundTint="#86ffffff"
        android:text="AZEITONA"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_mussarela"
        android:layout_width="165dp"
        android:layout_height="90dp"
        android:layout_alignBottom="@+id/button_codorna"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:backgroundTint="#86ffffff"
        android:text="MUSSARELA"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_provolone"
        android:layout_width="165dp"
        android:layout_height="90dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_codorna"
        android:backgroundTint="#86ffffff"
        android:text="PROVOLONE"
        android:textColor="#000000"
        android:textSize="22sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_palmito"
        android:layout_width="165dp"
        android:layout_height="90dp"
        android:layout_alignBottom="@+id/button_provolone"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:backgroundTint="#86ffffff"
        android:text="PALMITO"
        android:textColor="#000000"
        android:textSize="@dimen/texto_palmito"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="150dp"
        android:layout_height="130dp"
        android:layout_below="@+id/button_provolone"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="47dp"
        android:backgroundTint="#86ffffff"
        android:src="@drawable/e" />

    <Button
        android:id="@+id/buttonPetiscoVoltar"
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/abc_vector_test" />

</RelativeLayout>
