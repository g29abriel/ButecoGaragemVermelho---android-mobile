<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="butecogaragem.cursoandroid.com.butecogaragem.TelaFritas"
    android:background="#383333">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="106dp"
        android:layout_height="86dp"
        android:layout_alignBottom="@+id/button_canoa"
        android:layout_alignEnd="@+id/button_coxinha"
        android:layout_alignRight="@+id/button_coxinha"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:src="@drawable/f" />

    <Button
        android:id="@+id/button_anel"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_kibe"
        android:backgroundTint="#86ffffff"
        android:text="ANEL DE CEBOLA"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_polenta"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/button_bolinha_queijo"
        android:layout_alignTop="@+id/button_kibe"
        android:backgroundTint="#86ffffff"
        android:text="POLENTA"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_almondega"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_calabresa"
        android:backgroundTint="#86ffffff"
        android:text="ALMÃ”NDEGA"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_torresmo"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/button_bolinha_queijo"
        android:layout_alignStart="@+id/button_bolinha_queijo"
        android:layout_below="@+id/button_polenta"
        android:backgroundTint="#86ffffff"
        android:text="TORRESMO"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_calabresa"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_anel"
        android:backgroundTint="#86ffffff"
        android:text="CALABRESA"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_bolinha_queijo"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/button_panceta"
        android:layout_alignStart="@+id/button_panceta"
        android:layout_below="@+id/button_torresmo"
        android:backgroundTint="#86ffffff"
        android:text="BOLINHA DE QUEIJO"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_coxinha"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button_almondega"
        android:backgroundTint="#86ffffff"
        android:text="COXINHA"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_panceta"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/button_bolinha_queijo"
        android:backgroundTint="#86ffffff"
        android:text="PANCETA"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_kibe"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:backgroundTint="#86ffffff"
        android:text="KIBE RECHEADO"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_canoa"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/button_batata_bacon"
        android:backgroundTint="#ccda13"
        android:text="BATATA CANOA"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_batata"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignBottom="@+id/button_coxinha"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:backgroundTint="#ccda13"
        android:text="BATATA"
        android:textSize="@dimen/texto_batata"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button_batata_bacon"
        android:layout_width="165dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/button_batata"
        android:backgroundTint="#ccda13"
        android:text="BATATA C/ CHEDDAR E BACON"
        android:textSize="15sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/buttonFritasVoltar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button_anel"
        android:layout_marginBottom="20dp"
        android:background="@drawable/abc_vector_test" />

</RelativeLayout>
